module pack_unpack;
  integer i, j;
  reg [0:0] a;
  reg [0:0] b;
  
  initial begin
    i = 2;
    j = 0;
    {a, b} = i;
    
    $display("Unpack: A:%b, B:%b, Input:%d", a, b, i);
    
    j = {a, b};
    $display("Packed: A:%b, B:%b, Input:%d", a, b, i);
    
  end
endmodule
